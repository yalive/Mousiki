CREATE TABLE IF NOT EXISTS channel_search_result (
`id` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
`channel_id` TEXT NOT NULL,
`name` TEXT NOT NULL,
`urlImage` TEXT NOT NULL,
`query` TEXT NOT NULL
);

CREATE UNIQUE INDEX IF NOT EXISTS `index_channel_search_result_channel_id` ON `channel_search_result` (`channel_id`);

insert:
INSERT OR REPLACE INTO channel_search_result(channel_id, name, urlImage,query)
VALUES ?;

getResultForQuery:
SELECT * FROM channel_search_result WHERE `query`= ?;